{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "src/*": ["./*"],
      "@/*": ["./*"],
      "src": ["./"],
      "@": ["./"]
    },
    "plugins": [
      { "transform": "tsx-control-statements/transformer", "type": "config", "after": false },
      { "transform": "./config/ts-plugin-mmlpx", "type": "config" },
      { "transform": "typescript-transform-paths" },
      { "transform": "typescript-plugin-styled-components", "type": "config", "after": false },
      { "transform": "@avensia-oss/ts-transform-hoist-objects-in-props", "type": "program", "after": false },
      { "transform": "ts-nameof", "type": "raw", "after": false }, 
      { "transform": "ts-optchain/transform", "after": false },
      { "transform": "ts-transformer-keys/transformer" },
      { "transform": "ts-import-plugin", "type": "config", 
        "libraryName": "antd", 
        "libraryDirectory": "lib", 
        "style": "css" 
      },
      { "transform": "ts-import-plugin", "type": "config",
        "libraryName": "rxjs/operators",
        "libraryDirectory": "../_esm5/internal/operators",
        "camel2DashComponentName": false,
        "transformToDefaultImport": false
      },
      { "transform": "ts-import-plugin", "type": "config",
        "libraryName": "rxjs",
        "libraryDirectory": "_esm5/internal/observable",
        "camel2DashComponentName": false,
        "transformToDefaultImport": false
      },
      { "transform": "ts-import-plugin", "type": "config", 
        "style": false,
        "libraryName": "lodash",
        "libraryDirectory": null,
        "camel2DashComponentName": false 
      },
      { "transform": "ts-import-plugin", "type": "config", 
        "style": false,
        "libraryName": "@material-ui/core",
        "libraryDirectory": "",
        "camel2DashComponentName": false 
      }, 
      { "transform": "ts-import-plugin", "type": "config", 
        "libraryName": "element-ui",
        "libraryDirectory": "lib",
        "camel2DashComponentName": true,
        "style": false
      }, 
      { "transform": "ts-import-plugin", "type": "config", 
        "libraryName": "element-react",
        "libraryDirectory": "dist/npm/es5/src",
        "camel2DashComponentName": true,
        "style": false
      }
    ],
    "lib": ["es6", "dom", "dom.iterable", "scripthost"],
    "typeRoots": [
      "node_modules/@types",
      "./src/global"
    ],
    "module": "esnext",
    "target": "es5",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmit": true,
    "noResolve": false,
    "allowUnreachableCode": false,
    "jsx": "react",
    "downlevelIteration": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules",
    "devlib",
    "build",
    "dist",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest"
  ],
  "include": [
    "src"
  ]
}
