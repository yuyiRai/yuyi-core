{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "lib",
    "allowJs": false,
    "declarationMap": true,
    "outDir": "types",
    "rootDirs": [
      ".",
      "package/yuyi-core-utils"
    ],
    "baseUrl": "./",
    "paths": {
      "@yuyi/*": [
        "package/yuyi-core-*/src"
      ],
      "@/*": [
        "./src/*"
      ],
      "*": [
        "node_modules",
        "package"
      ]
    },
    "plugins": [
      {
        "transform": "tsx-control-statements/transformer",
        "type": "config",
        "after": false
      },
      {
        "transform": "yuyi-core-env/config/ts-plugin-mmlpx",
        "type": "config"
      },
      {
        "transform": "typescript-transform-paths",
        "after": false
      },
      {
        "transform": "typescript-transform-paths",
        "after": false,
        "afterDeclarations": true
      },
      {
        "transform": "typescript-plugin-styled-components",
        "type": "config",
        "after": false
      },
      {
        "transform": "@avensia-oss/ts-transform-hoist-objects-in-props",
        "type": "program",
        "after": false
      },
      {
        "transform": "ts-nameof",
        "type": "raw",
        "after": false
      },
      {
        "transform": "ts-nameof",
        "type": "raw",
        "afterDeclarations": true
      },
      {
        "transform": "ts-optchain/transform"
      },
      {
        "transform": "ts-optchain/transform",
        "afterDeclarations": true
      },
      {
        "transform": "ts-transformer-keys/transformer"
      },
      {
        "transform": "ts-transformer-keys/transformer",
        "afterDeclarations": true
      },
      {
        "transform": "ts-import-plugin",
        "type": "config",
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": "css"
      },
      {
        "transform": "ts-import-plugin",
        "type": "config",
        "libraryName": "rxjs/operators",
        "libraryDirectory": "../_esm5/internal/operators",
        "camel2DashComponentName": false,
        "transformToDefaultImport": false
      },
      {
        "transform": "ts-import-plugin",
        "type": "config",
        "libraryName": "rxjs",
        "libraryDirectory": "_esm5/internal/observable",
        "camel2DashComponentName": false,
        "transformToDefaultImport": false
      },
      {
        "transform": "ts-import-plugin",
        "type": "config",
        "style": false,
        "libraryName": "lodash",
        "libraryDirectory": null,
        "camel2DashComponentName": false
      },
      {
        "transform": "ts-import-plugin",
        "type": "config",
        "style": false,
        "libraryName": "lodash",
        "libraryDirectory": null,
        "camel2DashComponentName": false,
        "afterDeclarations": true
      },
      {
        "transform": "ts-import-plugin",
        "type": "config",
        "style": false,
        "libraryName": "@material-ui/core",
        "libraryDirectory": "",
        "camel2DashComponentName": false
      },
      {
        "transform": "ts-import-plugin",
        "type": "config",
        "libraryName": "element-ui",
        "libraryDirectory": "lib",
        "camel2DashComponentName": true,
        "style": false
      },
      {
        "transform": "ts-import-plugin",
        "type": "config",
        "libraryName": "element-react",
        "libraryDirectory": "dist/npm/es6/src",
        "camel2DashComponentName": true,
        "style": false
      }
    ]
  }
}