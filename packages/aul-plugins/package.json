  "bin": {
    "clua": ".bin/clua.bat",
    "ciuplua": ".bin/build.bat",
    "cupx": ".bin/upx.bat"
  },
  "scripts": {
    "dev": "yarn gulp watch-scripts",
    "start": "lua src/main.lua",
    "build": "yarn gulp build-bin",
    "build2": "yarn gulp build-script",
    "build:main": "yarn gulp build-main",
    "build:lib": "yarn gulp build-lib",
    "build:luac": "luac -s -o lib/main.luac lib/main.lua",
    "gulp": "gulp --cwd . -f ./scripts/gulp.ts",
    "iupdll": "ciuplua iup/olecontrol_browser.wlua iup/olecontrol_browser.exe",
    "dist": "ciuplua lib/main.luac build/main.exe",
    "upx": "cupx build/main.exe min.exe",
    "upx:help": "cupx -h",
    "test": "CHCP 65001 && yarn test:native",
    "test:native": "lua ./test.lua \"([？！。」]+)\""
  },
