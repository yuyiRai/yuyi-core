{
  "name": "@yuyi919/ts-api-docs",
  "version": "0.0.14",
  "description": "",
  "keywords": [
    "mobx",
    "lodash",
    "typescript"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "lib",
    "bin.js",
    "config",
    "schemas",
    "yaml"
  ],
  "author": "yuyiRai",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "bin": {
    "yuyi-doc": "bin.js"
  },
  "scripts": {
    "postpublish": "yarn global add @yuyi919/ts-api-docs@latest",
    "build": "tsdx build",
    "build:bin": "yuyi-builder -i ./bin.js -n ts-api-docs",
    "test": "cross-env CWD=D:/WorkSpace/@yuyi/packages/utils DOC_PROJECT_NAME=@yuyi919/utils DOC_MAIN_POINT=lib DOC_DEPENDS=core-decorators,rxjs yuyi-gulp --gulpfile D:/WorkSpace/@yuyi/packages/ts-api-docs/config/gulpfile.ts"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/benchmark": "^1.0.31",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "coveralls": "^3.0.2",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "lint-staged": "^8.0.0",
    "lodash-decorators": "^6.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "mobx": "^4.15.0",
    "mobx-utils": "^5.5.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.8",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.4",
    "typescriptcollectionsframework": "^4.4.10"
  },
  "gitHead": "bdec46a35774a06a8e7d08cc34061f0b56077bb2",
  "dependencies": {
    "@microsoft/api-extractor": "^7.7.13",
    "@microsoft/api-extractor-model": "^7.7.11",
    "@yuyi919/gulp-awesome": "^0.0.12",
    "@yuyi919/shell-exec-awesome": "^0.0.5",
    "colors": "^1.4.0",
    "cross-env": "^5.2.1",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-convert": "^0.2.0",
    "gulp-shell": "^0.7.1",
    "gulp-yml-merge": "^0.0.2",
    "lodash": "^4.17.15"
  }
}
