{
  "name": "@yuyi919/ts-api-docs",
  "version": "0.0.61",
  "description": "",
  "keywords": [
    "mobx",
    "lodash",
    "typescript"
  ],
  "private": true,
  "main": "dist/index.js",
  "types": "lib/index.export.d.ts",
  "typings": "lib/index.export.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "author": "yuyiRai",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "bin": {
    "yuyi-doc": "bin.js"
  },
  "scripts": {
    "test": "cross-env CWD=D:/WorkSpace/@yuyi/packages/utils DOC_PROJECT_NAME=@yuyi919/utils DOC_MAIN_POINT=lib DOC_DEPENDS=core-decorators,rxjs yuyi-gulp --gulpfile D:/WorkSpace/@yuyi/packages/ts-api-docs/config/gulpfile.ts"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
