  "scripts": {
    "start": "tsdx watch",
    "build": "yarn build:ts && yarn build:lib",
    "build:p": "yarn build && yarn test",
    "build:lib": "tsc -p ./tsconfig.lib.json",
    "build:ts": "tsc --build",
    "test": "tsdx test",
    "ttsc": "ttsc",
    "test:complie": "yuyi exec ./test/blah.test.ts",
