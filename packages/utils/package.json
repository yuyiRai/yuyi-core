{
  "name": "@yuyi919/utils",
  "version": "0.0.70",
  "description": "",
  "keywords": [
    "mobx",
    "lodash",
    "typescript"
  ],
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.export.d.ts",
  "typings": "lib/index.export.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "author": "yuyiRai",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "doc": "node \"D:/WorkSpace/@yuyi/packages/ts-api-docs/bin.js\" lib --depends core-decorators,rxjs,typescriptcollectionsframework",
    "doc:serve": "ttsc --build && cross-env NODE_ENV=development yarn doc --port 8009 --mode serve ",
    "gulp": "yuyi-gulp --gulpfile ./config/gulpfile.ts",
    "td": "yuyi-gulp --gulpfile ./config/devoutput.js",
    "td2": "yarn td watch",
    "ttsc": "ttsc",
    "tsc": "ttsc --build",
    "tsc:watch": "ttsc --build --watch",
    "presave": "yarn api",
    "addTemplate": "nuget install memberpage -OutputDirectory config/template",
    "markdown": "api-documenter markdown -i ./etc -o ./document/articles/docs",
    "yaml": "api-documenter yaml -i ./etc -o ./document/src",
    "deploy-docs": "gh-pages -d docs",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist/**",
    "rollup": "ts-node -O \"{ \\\"module\\\": \\\"CommonJS\\\" }\" -T ./config/exec-rollup.ts",
    "build": "ttsc --build && cross-env NODE_ENV=production yarn rollup",
    "build3": "ttsc --build && tsdx build --noClean",
    "build4": "tsdx build -h",
    "start": "cross-env NODE_ENV=development yarn rollup -w",
    "benchmark": "yuyi -c ttypescript exec test/benchmark/forEach.ts",
    "test": "jest",
    "test:cover": "jest --coverage --all",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "pub": "npm publish --access=public",
    "rimraf": "rimraf"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/benchmark": "^1.0.31",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "argparse": "^1.0.10",
    "benchmark": "^2.1.4",
    "colors": "^1.4.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "dayjs": "^1.8.15",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-convert": "^0.2.0",
    "gulp-shell": "^0.7.1",
    "gulp-yml-merge": "^0.0.2",
    "immer": "^5.0.0",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "mobx": "^4.15.0",
    "mobx-utils": "^5.5.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^1.1.12",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-visualizer": "^3.0.0",
    "rxjs": "^6.5.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.4",
    "typescriptcollectionsframework": "^4.4.10"
  },
  "gitHead": "5efd00d519e197a9e9ec8e67985fc1f67ae736f0"
}
