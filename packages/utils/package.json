{
  "name": "@yuyi919/utils",
  "version": "0.0.19",
  "description": "",
  "keywords": [
    "mobx",
    "lodash",
    "typescript"
  ],
  "private": false,
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "types",
    "lib"
  ],
  "author": "yuyiRai",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "td": "gulp -f config/devoutput.js",
    "td2": "gulp -f config/devoutput.js watch",
    "build:api": "ttsc --build && yarn api && yarn markdown && yarn yaml && rimraf ./tmp && rimraf ./temp",
    "api:main": "api-extractor run --local --verbose",
    "api:rx": "api-extractor run -c config/api-extractor-rxjs.json --local --verbose",
    "api:depend": "yarn api:rx",
    "api:fix": "gulp --require @yuyi919/env/config/run-ts.js --cwd . -f ./config/gulpfile.ts api:fix",
    "doc:fix": "gulp --require @yuyi919/env/config/run-ts.js --cwd . -f ./config/gulpfile.ts doc:fix",
    "api": "yarn api:main && yarn api:rx && yarn api:fix",
    "api:all": "gulp --require @yuyi919/env/config/run-ts.js --cwd . -f ./config/gulpfile.ts",
    "tsc": "ttsc --build",
    "tsc:watch": "ttsc --build --watch",
    "doc:output": "yarn tsc && yarn api:all && yarn docfx:serve",
    "presave": "yarn api",
    "addTemplate": "nuget install memberpage -OutputDirectory config/template",
    "markdown": "api-documenter markdown -i ./etc -o ./document/articles/docs",
    "yaml": "api-documenter yaml -i ./etc -o ./document/src",
    "docfx": "yarn docfx:build",
    "docfx:build": "rimraf ./docs/**/* && docfx build ./document/docfx.json",
    "docfx:serve": "rimraf ./docs/**/* && docfx ./document/docfx.json --serve",
    "docs": "yuyidocs ../..",
    "docs:full": "yuyidocs ../.. -i",
    "docs:watch": "yuyidocs ../.. -w",
    "predeploy-docs": "yarn run docs:full",
    "deploy-docs": "gh-pages -d docs",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist/**/*.js && rimraf dist/**/*.map && rimraf dist/**/*.d.ts",
    "build": "ttsc --build && cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "start": "cross-env NODE_ENV=development rollup -c rollup.config.ts -w",
    "start:all": "yarn start && yarn run docs:watch",
    "test": "jest",
    "test:cover": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "pub": "npm publish --access=public",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/benchmark": "^1.0.31",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^24.0.16",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.9",
    "benchmark": "^2.1.4",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "dayjs": "^1.8.15",
    "gulp": "^4.0.2",
    "gulp-convert": "^0.2.0",
    "gulp-shell": "^0.7.1",
    "gulp-yml-merge": "^0.0.2",
    "husky": "^2.3.0",
    "immer": "^3.1.3",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^8.0.0",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "mobx": "4.9.4",
    "mobx-utils": "^5.4.1",
    "prettier": "^1.18.2",
    "prompt": "^1.0.0",
    "replace-in-file": "^4.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.18.0",
    "rollup-plugin-babel-minify": "^9.0.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-dts": "^1.1.5",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.22.1",
    "rollup-plugin-wasm": "^3.0.0",
    "rxjs": "^6.5.2",
    "semantic-release": "^15.13.17",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-is-kind": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "3.5.3",
    "typescriptcollectionsframework": "^4.4.10"
  },
  "gitHead": "1c61cc573067f9c157bf8e1c9988000968a0ab5e"
}
