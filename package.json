{
  "name": "yuyi-core",
  "version": "0.10.29",
  "description": "yuyi",
  "license": "MIT",
  "private": true,
  "main": "package/yuyi-core-night/lib/cjs/index.js",
  "module": "package/yuyi-core-night/lib/esm/index.js",
  "typings": "package/yuyi-core-night/lib/esm/index.d.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyiRai/yuyi-core.git"
  },
  "author": "yuyiRai<yuyi919606952@foxmail.com>",
  "bugs": {
    "url": "https://github.com/yuyiRai/yuyi-core/issues"
  },
  "sideEffects": false,
  "homepage": "https://github.com/yuyiRai/yuyi-core#readme",
  "workspaces": [
    "package/*"
  ],
  "scripts": {
    "prepackagecommit": "lerna exec --no-private -- git add .",
    "packagecommit": "lerna exec --no-private -- git commit -a -m 'NEXT'",
    "commit": "git add . && git commit -a -m 'NEXT'",
    "allcommit": "yarn packagecommit && git add . && git commit -a -m 'NEXT'",
    "serve": "yarn workspace yuyi-core-react-demo run start",
    "dev": "yarn workspace yuyi-core-utils run dev",
    "dev2": "yarn workspace yuyi-core-night run dev",
    "build": "yarn run build-utils && yarn run build-night",
    "build-night": "yarn workspace yuyi-core-night run build",
    "start-night": "yarn workspace yuyi-core-night run start",
    "build-utils": "yarn workspace yuyi-core-utils run build"
  },
  "devDependencies": {
    "lerna": "^3.14.1"
  }
}
