{
  "name": "yuyi-core",
  "version": "0.10.29",
  "description": "yuyi",
  "license": "MIT",
  "private": true,
  "main": "package/yuyi-core-night/lib/cjs/index.js",
  "module": "package/yuyi-core-night/lib/esm/index.js",
  "typings": "package/yuyi-core-night/lib/esm/index.d.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyiRai/yuyi-core.git"
  },
  "author": "yuyiRai<yuyi919606952@foxmail.com>",
  "bugs": {
    "url": "https://github.com/yuyiRai/yuyi-core/issues"
  },
  "sideEffects": false,
  "homepage": "https://github.com/yuyiRai/yuyi-core#readme",
  "workspaces": [
    "package/*"
  ],
  "scripts": {
    "allcommit": "lerna exec --no-private -- yuyienv ../.. -p && yarn",
    "commit": "yuyienv .",
    "serve": "yarn workspace yuyi-core-react-demo run start",
    "serve:vue": "yarn workspace yuyi-core-vue-demo run serve",
    "ts": "node -r ./package/yuyi-core-env/config/run-ts.js -r ./scripts/publish.ts",
    "pub": "yarn commit && lerna publish && yarn run allcommit",
    "start": "lerna run --no-private dev",
    "start:utils": "yarn workspace yuyi-core-utils run start",
    "start:night": "yarn workspace yuyi-core-night run start",
    "dev": "lerna run --no-private dev",
    "dev:utils": "yarn workspace yuyi-core-utils run dev",
    "dev:night": "yarn workspace yuyi-core-night run dev",
    "build": "yarn run build-utils && yarn run build-night",
    "build-night": "yarn workspace yuyi-core-night run build",
    "start-night": "yarn workspace yuyi-core-night run start",
    "build-utils": "yarn workspace yuyi-core-utils run build"
  },
  "devDependencies": {
    "lerna": "^3.14.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
